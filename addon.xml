<?xml version="1.1" encoding="UTF-8" standalone="yes"?>
<addon id="script.openviewselectdialog" name="Open View Select Dialog" version="0.1.0" provider-name="fnord12">
	<requires>
		<import addon="xbmc.python" version="3.0.0"/>
	</requires>

	
<extension point="xbmc.python.script" library="default.py">
		<provides>executable</provides>
	</extension>
	
	
	
	<extension point="xbmc.addon.metadata">
		<summary lang="en_GB">For use with Confnordance skin.  Opens a dialog for View select.</summary>
		<description lang="en_GB">Opens a dialog for View select instead of having to click to cycle through each option.</description>
		<language></language>
		<platform>all</platform>
		
		
    
</extension>
</addon>



